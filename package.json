{
  "name": "arsad",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://majidzeno.github.io/arsad",
  "dependencies": {
    "axios": "^0.17.1",
    "google-map-react": "^0.29.0",
    "lodash": "^4.17.4",
    "node-sass": "^4.7.2",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-fontawesome": "^1.6.1",
    "react-google-map": "^3.1.1",
    "react-google-maps-loader": "^4.1.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "react-sparklines": "^1.7.0",
    "redux": "^3.7.2",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "youtube-api-search": "0.0.5"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/scss/ -o src/css/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/scss/ -o src/css/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build&&gh-pages -d build"
  },
  "devDependencies": {
    "gh-pages": "^1.1.0",
    "npm-run-all": "^4.1.2",
    "redux-devtools": "^3.4.1"
  }
}
